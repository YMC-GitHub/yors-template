{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
  ],
  "overrides": [
      {
          "env": {
              "node": true
          },
          "files": [
              ".eslintrc.{js,cjs}"
          ],
          "parserOptions": {
              "sourceType": "script"
          }
      }
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
    "prefer-const":["off"],
    "no-useless-escape":["off"],
    "@typescript-eslint/no-unused-vars":["warn"],
    "@typescript-eslint/no-explicit-any":["off"],
    "@typescript-eslint/ban-ts-comment":["off"]
  }
}
